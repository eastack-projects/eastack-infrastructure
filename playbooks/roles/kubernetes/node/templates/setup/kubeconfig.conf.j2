apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority-data: {{ int_cert['content'] }}
    server: https://{{ hostvars['k8s-server'].ansible_host }}:6443
  name: default
contexts:
- context:
    cluster: default
    namespace: default
    user: default
  name: default
current-context: default
users:
- name: default
  user:
    client-certificate-data: {{ kube_proxy_kube_apiserver_client_cert['content'] }}
    client-key-data: {{ kube_proxy_kube_apiserver_client_key['content'] }}
